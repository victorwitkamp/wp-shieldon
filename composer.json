{
  "name": "victorwitkamp/wp-shieldon",
  "description": "WP Shieldon is based on Shieldon library, a PHP library that provides anti-scraping and online session control for your WordPress sites.",
  "keywords": [
    "antiscriping",
    "wordpress-security",
    "wordpress-plugin",
    "wordpress-security-plugin"
  ],
  "homepage": "https://github.com/victorwitkamp/wp-shieldon",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Victor Witkamp",
      "email": "info@victorwitkamp.nl",
      "homepage": "https://victorwitkamp.nl/"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-curl": "*",
    "monolog/monolog": "^3.5",
    "ext-gd": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "wp-coding-standards/wpcs": "3.0",
    "squizlabs/php_codesniffer": "@stable"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/victorwitkamp/shieldon"
    }
  ]
}